			function productClick(list, position)
			{
				var positionID = 'position'+position;
				dataLayer.push({
				'event': 'productClick',
				'ecommerce': {
				  'click': {
					'actionField': {'list': ListProduct[list][positionID]['list']},
					'products': [{
					  'name': ListProduct[list][positionID]['name'],
					  'id': ListProduct[list][positionID]['id'],
					  'price': ListProduct[list][positionID]['price'],
					  'brand': ListProduct[list][positionID]['brand'],
					  'category': ListProduct[list][positionID]['category'],
					  'position': ListProduct[list][positionID]['position']
					 }]
				   }
				 }
				});
			}
