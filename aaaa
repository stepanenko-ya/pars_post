<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script type="text/javascript">
			$(document).ready(function() {
				var owl = $("#produktyrotator");
				var counter = $('#losowe-produkty-widoczne #js-current');

				owl.owlCarousel({
				items : 1,
					itemsDesktop : [1000,1],
					itemsDesktopSmall : [900,3],
					itemsTablet: [600,2],
					itemsMobile : [300,1],
					autoPlay : true,
					pagination : false,
					afterInit: function(){
						counter.html((this.owl.currentItem)+1);
						$('#losowe-produkty-widoczne #js-from').html(this.owl.owlItems.length);
					},
					afterMove : function(){
						counter.html((this.owl.currentItem)+1);
					},
				  });

				  $("#losowe-produkty-widoczne .def-next").click(function(){
					owl.trigger('owl.next');
				  });
				  $("#losowe-produkty-widoczne .def-prev").click(function(){
					owl.trigger('owl.prev');
				  });
				});
		</script>
