			$(document).ready(function(){
				var widok = getCookie('widok');
				$('#js-kafle').bind('click', function(){
					$(this).addClass('active');
					$('.katalog-lista .produkt').parent().addClass('medium-6');
					$('#js-lista').removeClass('active');
					$('.katalog-lista .produkt').parent().removeClass('medium-12');
					setCookie('widok', 'kafle');
				});

				$('#js-lista').bind('click', function(){
					$(this).addClass('active');
					$('.katalog-lista .produkt').parent().addClass('medium-12');
					$('#js-kafle').removeClass('active');
					$('.katalog-lista .produkt').parent().removeClass('medium-6');
					setCookie('widok', 'lista');
				});

				var breadcrumbHeight = $('.page_path.row').outerHeight();
				var aside = $('aside');
				aside.css('margin-top', -breadcrumbHeight);

				if(!widok) widok = 'lista';
				$('#js-'+widok).trigger('click');
			});
		</script>
