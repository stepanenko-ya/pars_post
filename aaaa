<script type="text/javascript">
														$(document).ready(function(){
															$(".menu-icon").click(function(){
																$(".top-menu").toggleClass( "active" );
															});
															$("#opinie-ocena").text(opinieOcena);
															$("#opinie-ilosc").text(opinieIlosc);
