<script type="text/javascript">
	$(document).ready(function(){

		$(".koszyk, .przechowalnia, .porownaj").click(function(){
			var id = $(this).attr("name");
			var vendo = "";
			var producent = "";
			var id_vendo = 0;
			var nazwa_produktu = "";
			var data = new Array();
			var akcja = id.substr(0, 1);
			id = id.substr(1, id.length);

			var idp = id;
			data = id.split("-");
			id = data[2];
			vendo = data[0];
			producent = data[1];
			id_vendo = data[3];
			nazwa_produktu = data[4];

			$("#id_produktu").val(id);
			$("#isvendo").val(vendo);
			$("#id_producenta").val(producent);
			$("#id_vendo").val(id_vendo);
			$("#nazwa_produktu").val(nazwa_produktu);

			if(akcja == 'c')
				akcja = 'porownanie';
			else if(akcja == 's')
				akcja = 'przechowalnia';
			else
				akcja = 'koszyk';

			if (akcja !== 'koszyk') {
				$("#form_akcja").val(akcja);
				$("#katalogForm").submit();
			}
		});

		$("#pagerSize").change(function(){
			$("#SklepKatalogSortowanie").submit();
		});

		$("#select_sort").change(function(){
			$("#sort").val($(this).val());
			$("#SklepKatalogSortowanie").submit();
		});

		$(".aktualnosciPager a").click(function(){
			$("#pagerFrom").val($(this).attr('id'));
			$("#SklepKatalogSortowanie").submit();
		});
	});
</script>
