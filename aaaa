														$('#top-infolinia-dzien-' + lp_dnia).css("font-weight", "bold");
														$('#top-infolinia-mobile-dzien-' + lp_dnia).css("font-weight", "bold");
														$("#query").focus(function() {
															$(".top-infolinia-dni-pracy").removeClass("show");
														});
														$(".infolinia-anchor").focusout(function() {
															$(".top-infolinia-dni-pracy").removeClass("show");
														});
														var d = new Date();
														if(d < new Date(2020, 11, 24, 14, 0, 0) || d > new Date(2020, 11, 28, 8, 0, 0))
														{
															$(".infolinia-anchor").click(function(){
																$(".top-infolinia-dni-pracy").toggleClass("show", "none");
															});
														}
														  $("body").click(function(e) {
															  let click_class = $(e.target).attr('class') == undefined ? "" : $(e.target).attr('class');
															  let click_id = $(e.target).attr('id');
															if((click_id !== "infolinia-text" && click_id !== "infolinia-mobile-text") && (click_class.indexOf("top-info") === -1)) {
																$(".top-infolinia-dni-pracy").removeClass("show");
															}
														});
													});
