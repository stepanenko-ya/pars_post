				var lp_dnia = d.getDay();
				if(d >= new Date(2020, 11, 24, 14, 00, 00) && d <= new Date(2020, 11, 28, 8, 00, 00))
				{
					$('#infolinia-stopka-text').text('Infolinia czynna 28/12 od 8.00 ');
