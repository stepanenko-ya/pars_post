		function closeLoader()
		{
			$('.preloader-tlo, .preloader-kontener').remove();
			$('#query').focus();
		}
		function showLoader()
		{
			var element = $('body');
			var height = document.body.clientHeight;
			var offsetTop = 300;
			var local = localStorage.getItem('car_search_data_key');
			if(local){
				local = JSON.parse(local);
				var id = local.id;
				id = id.split('-');
				document.getElementById('idCar').value = id[2] ? id[2] : null;
			}
			element.append('<div class="preloader-tlo"></div>');
			if(($('#query').attr("value") == 'Wpisz numer lub nazwę części') || ($('#query').attr("value") == '')){
				$('#query').attr("value","")
				element.append('<div class="preloader-kontener do-zamkniecia" style="top: '+offsetTop+'px"><div class="preloader-txt"><span>Wpisz wyszukiwaną frazę</span></div><br /><input type="button" value="Zamknij" onclick="closeLoader();" id="closeLoader" class="gsc-search-button zamknij"></div>');
				$('#closeLoader').focus();
				return false;
			}
			else {
				element.append('<div class="preloader-kontener" style="top: '+offsetTop+'px"><div class="preloader"><span class="fs-lightbox-loading_icon"></span></div><div class="preloader-txt"><span>Przeszukujemy dla Ciebie bazę ponad 2 milionów części !</span></div></div>');
				return true;

			}
		}
	</script>
