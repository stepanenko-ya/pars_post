<script type="text/javascript">
	$(document).foundation({
		abide : {
		   validators: {
			 notEmptyInvoice: function(el, required, parent) {
				 if ($('#fv').prop('checked')) {
					if ($(el).val() == '')
					{
						return false;
					}
				 }
				 return true;
			 },
			 notEmptyIfPrivate: function(el, required, parent) {
				if ($('#fv').prop('checked') && $('#odbiorca_prywatny').prop('checked')) {
					$("#fv_firma, #fv_nip").parent().removeClass('error');
					$("label[for='fv_firma'], label[for='fv_nip']").removeClass('error');
					if ($(el).val() == '')
					{
						return false;
					}
				}
				return true;
			},
			notEmptyIfCompany: function(el, required, parent) {
			   if ($('#fv').prop('checked') && $('#odbiorca_firma').prop('checked')) {
				   $("#fv_imie, #fv_nazwisko").parent().removeClass('error');
				   $("label[for='fv_imie'],  label[for='fv_nazwisko']").removeClass('error');
				  if ($(el).val() == '')
 				  {
 					  return false;
 				  }
			   }

			   return true;
		   }
		   }
 		}
	});
	</script>
