<script type="text/javascript">
				$(document).ready(function() {
					let widthEm = $(window).width() / parseFloat($("body").css("font-size"));
					if(widthEm < 40) {
						var owl = $("#logo-list-3");
						if(owl.children('.logo').length > 3) {
							owl.owlCarousel({
							items : 3,
								itemsDesktop : [1000,3],
								itemsDesktopSmall : [900,3],
								itemsTablet: [600,3],
								itemsMobile : [400,3],
								autoPlay : false,
								pagination : false,
								addClassActive : true,
								theme: 'owl-theme logo-list-theme',
								afterUpdate: function(){
									if(this.owl.owlItems.length <= this.owl.visibleItems.length)
									{
										$("#logo-list-3").parent().find(".next").addClass('disabled');
										$("#logo-list-3").parent().find(".prev").addClass('disabled');
									}
									else
									{
										$("#logo-list-3").parent().find(".next").removeClass('disabled');
										$("#logo-list-3").parent().find(".prev").removeClass('disabled');
									}
								},
								afterInit: function(){
									if(this.owl.owlItems.length <= this.owl.visibleItems.length)
									{
										$("#logo-list-3").parent().find(".next").addClass('disabled');
										$("#logo-list-3").parent().find(".prev").addClass('disabled');
									}

									$("#logo-list-3").parent().find(".next").click(function(){
										owl.trigger('owl.next');
									});
									$("#logo-list-3").parent().find(".prev").click(function(){
										owl.trigger('owl.prev');
									});
								},
							});
						}
						else {
							$("#logo-list-3").parent().addClass('disabled');
						}
						var owl2 = $("#logo-list-2");
						if(owl2.children('logo').length > 3) {

							owl2.owlCarousel({
							items : 3,
								itemsDesktop : [1000,3],
								itemsDesktopSmall : [900,3],
								itemsTablet: [600,3],
								itemsMobile : [400,3],
								autoPlay : false,
								pagination : false,
								addClassActive : true,
								theme: 'owl-theme logo-list-theme',
								afterUpdate: function(){
									if(this.owl.owlItems.length <= this.owl.visibleItems.length)
									{
										$("#logo-list-2").parent().find(".next").addClass('disabled');
										$("#logo-list-2").parent().find(".prev").addClass('disabled');
									}
									else
									{
										$("#logo-list-2").parent().find(".next").removeClass('disabled');
										$("#logo-list-2").parent().find(".prev").removeClass('disabled');
									}
								},
								afterInit: function(){

									if(this.owl.owlItems.length <= this.owl.visibleItems.length)
									{
										$("#logo-list-2").parent().find(".next").addClass('disabled');
										$("#logo-list-2").parent().find(".prev").addClass('disabled');
									}

									$("#logo-list-2").parent().find(".next").click(function(){
										owl.trigger('owl.next');
									});
									$("#logo-list-2").parent().find(".prev").click(function(){
										owl.trigger('owl.prev');
									});
								},
							});
						}
						else {
							$("#logo-list-2").parent().addClass('disabled');
						}
					}
				});
			</script>
