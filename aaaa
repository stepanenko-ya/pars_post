	//			$(".blok_szukaj_czesci_zaawansowane").hover(
	//				function(){
	//					$(".blok_szukaj_czesci_zaawansowane").addClass("blok_szukaj_czesci_zaawansowane_wyroznik")
	//				},
	//				function(){
