<script type="text/javascript">
    $(document).ready(function() {
        var owl = $("#jq-produkty-w-promocji");

        owl.owlCarousel({
        items : 4,
            itemsDesktop : [1050,4],
            itemsDesktopSmall : [1024,3],
            itemsTablet: [640,1],
            itemsMobile : [400,1],
            autoPlay : true,
            pagination : false,
            addClassActive : true,
            afterUpdate: function(){
                if(this.owl.owlItems.length <= this.owl.visibleItems.length)
                {
                    $("#jq-produkty-w-promocji").parent().find(".next").addClass('disabled');
                    $("#jq-produkty-w-promocji").parent().find(".prev").addClass('disabled');
                }
                else
                {
                    $("#jq-produkty-w-promocji").parent().find(".next").removeClass('disabled');
                    $("#jq-produkty-w-promocji").parent().find(".prev").removeClass('disabled');
                }
            },
            afterInit: function(){

                if(this.owl.owlItems.length <= this.owl.visibleItems.length)
                {
                    $("#jq-produkty-w-promocji").parent().find(".next").addClass('disabled');
                    $("#jq-produkty-w-promocji").parent().find(".prev").addClass('disabled');
                }

                $("#jq-produkty-w-promocji").parent().find(".next").click(function(){
                    owl.trigger('owl.next');
                });
                $("#jq-produkty-w-promocji").parent().find(".prev").click(function(){
                    owl.trigger('owl.prev');
                });
            },
        });
    });
</script>
